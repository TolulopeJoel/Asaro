{
  "expo": {
    "name": "Àṣàrò",
    "slug": "asaro",
    "version": "2025.DEC",
    "orientation": "portrait",
    "icon": "./assets/images/foregroundImage.png",
    "scheme": "asaro",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.asaro.meditation"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/foregroundImage.png",
        "monochromeImage": "./assets/images/Vector.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.asaro.meditation"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/foregroundImage.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-router",
      "expo-sqlite",
      "expo-notifications",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "3eab02f1-b324-49fc-ae59-6d1bf4813176"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/3eab02f1-b324-49fc-ae59-6d1bf4813176",
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0
    }
  }
}