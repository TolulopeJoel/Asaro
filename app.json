{
  "expo": {
    "name": "Àṣàrò",
    "slug": "asaro",
    "version": "2025.DEC",
    "runtimeVersion": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/foregroundImage.png",
    "scheme": "asaro",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "android": {
      "permissions": [
        "SCHEDULE_EXACT_ALARM",
        "USE_EXACT_ALARM",
        "WAKE_LOCK",
        "RECEIVE_BOOT_COMPLETED",
        "android.permission.SCHEDULE_EXACT_ALARM",
        "android.permission.USE_EXACT_ALARM",
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.VIBRATE",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.WAKE_LOCK"
      ],
      "useNextNotificationsApi": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/foregroundImage.png",
        "monochromeImage": "./assets/images/kaka.png",
        "backgroundColor": "#E18F43"
      },
      "package": "com.asaro.meditation"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/foregroundImage.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#E18F43"
        }
      ],
      "expo-router",
      "expo-sqlite",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/foregroundImage.png",
          "color": "#E18F43",
          "sounds": [],
          "androidMode": "default",
          "androidCollapsedTitle": "Àṣàrò Reminder"
        }
      ],
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "3eab02f1-b324-49fc-ae59-6d1bf4813176"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/3eab02f1-b324-49fc-ae59-6d1bf4813176",
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0
    }
  }
}