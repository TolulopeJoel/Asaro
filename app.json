{
  "expo": {
    "name": "Àṣàrò",
    "slug": "asaro",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/àṣàròLogo.png",
    "scheme": "asaro",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.asaro.meditation"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/àṣàròLogo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.asaro.meditation"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/àṣàròLogo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-router",
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "3eab02f1-b324-49fc-ae59-6d1bf4813176"
      }
    }
  }
}
