{
  "cli": {
    "version": ">= 16.17.4",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "friends": {
      "distribution": "internal",
      "channel": "friends",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_NO_SOURCEMAP": "1"
      }
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "EXPO_NO_SOURCEMAP": "1"
      }
    }
  },
  "submit": {
    "production": {}
  }
}